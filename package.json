{
  "name": "react-move",
  "version": "1.2.0",
  "description": "Beautifully animate anything in react with interia or time + easing.",
  "license": "MIT",
  "homepage": "https://github.com/tannerlinsley/react-move#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerlinsley/react-move.git"
  },
  "keywords": [
    "react",
    "animate",
    "animation",
    "interpolation",
    "move",
    "time"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/",
    "react-move.js",
    "media/*.png",
    "index.d.ts"
  ],
  "types": "./index.d.ts",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps inline --presets=es2015,react,stage-2 --plugins=transform-object-assign",
    "watch": "onchange 'src/**' -i -- npm run build",
    "test": "standard",
    "umd": "rimraf react-move.js && webpack --config umd.webpack.js",
    "prepublish": "npm run build && npm run umd",
    "postpublish": "git push --tags",
    "docs": "cd docs && yarn && yarn start",
    "docs:build": "cd docs && yarn && yarn run build"
  },
  "dependencies": {
    "d3-ease": "^1.0.3",
    "d3-interpolate": "^1.1.5",
    "deep-equal": "^1.0.1",
    "performance-now": "^2.1.0",
    "raf": "^3.3.2"
  },
  "peerDependencies": {
    "@types/react": "^15.0.22",
    "react": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "onchange": "^3.0.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.6.1",
    "webpack": "^2.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "dist",
      "lib",
      "example",
      "react-move.js",
      "stories",
      "docs"
    ]
  }
}
